#: kivy 2.3.0


<AnalysisScreen@Screen>:
    inertial: inertial
    ekg: ekg
    eeg: eeg
    staging: staging
    hr_ekg: hr_ekg
    hr_ppg: hr_ppg
    hrv: hrv
    rr: rr
    temp: temp
    spo2: spo2
    pi: pi
    br: br

    BoxLayout:
        orientation: 'vertical'
        background_color: 1,1,1,1
        size_hint: (1, 1)

        BoxLayout:
            size_hint_y: 0.04
            spacing: 2
            CtaButton:
                text: 'Start'
                size_hint: None, None
                width: 200
                height: 30
                on_release: root.run()
            CtaButton:
                text: 'Stop'
                size_hint: None, None
                width: 200
                height: 30
                on_release: root.stop()
            Button:
                text: 'Back'
                size_hint: None, None
                width: 200
                height: 30
                on_release: app.root.ids['sm'].current = 'homepage_screen'

        BoxLayout:
            size_hint_y: 1
            spacing: 2
            size_hint_y: 0.93
            GridLayout:
                cols: 2
                rows: 2
                size_hint_y: 1
                spacing: 2
                StagingGraph:
                    id: staging
                EkgGraph:
                    id: ekg
                InertialGraph:
                    id: inertial
                EegGraph:
                    id: eeg

            GridLayout:
                cols: 1
                size_hint_x: 0.1
                spacing: 2
                HrEkgBox:
                    id: hr_ekg
                HrvBox
                    id: hrv
                RrBox:
                    id: rr
                TemperatureBox:
                    id: temp
                HrPpgBox:
                    id: hr_ppg
                Spo2Box:
                    id: spo2
                PiBox:
                    id: pi
                BrBox:
                    id: br








