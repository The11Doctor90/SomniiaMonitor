#: kivy 2.3.0


<AnalysisScreen@Screen>:
    inertial: inertial
    ekg: ekg
    eeg: eeg
    staging: staging
    hr_ekg: hr_ekg
    BoxLayout:
        orientation: 'vertical'
        background_color: 1,1,1,1

        BoxLayout:
            size_hint_y: None
            size:(100, 50)
            CtaButton:
                text: 'start'
                width: 250
                height: 50
                on_release: root.run()
            CtaButton:
                text: 'STOP'
                width: 250
                height: 50
                on_release: root.stop()

        GridLayout:
            cols: 2
            rows: 2
            StagingGraph:
                id: staging

            EegGraph:
                id: eeg

            InertialGraph:
                id: inertial

            EkgGraph:
                id: ekg

        GridLayout:
            cols: 4
            rows: 2
            spacing: 2
            HrEkgBox:
                id: hr_ekg

            HrvBox
                Label:
                    text: 'HRV'
                Label:
                    text: "2"

            RrBox:
                Label:
                    text: 'RR interval'
                Label:
                    text: "3"
            TemperatureBox:
                Label:
                    text: 'Temperature'
                Label:
                    text: "4"
            HrPpgBox:
                Label:
                    text: 'HR - PPG'
                Label:
                    text: "5"
            Spo2Box:
                Label:
                    text: 'SPO2'
                Label:
                    text: "6"
            PiBox:
                Label:
                    text: 'PI'
                Label:
                    text: "7"
            BrBox:
                Label:
                    text: 'BR'
                Label:
                    text: "8"




        BoxLayout
            size_hint_y: None
            size:(100, 50)
            Button:
                text: 'annulla'
                width: 250
                height: 50
                on_release: app.root.ids['sm'].current = 'homepage_screen'






