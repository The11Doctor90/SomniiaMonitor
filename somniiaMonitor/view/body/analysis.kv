#: kivy 2.3.0


<AnalysisScreen@Screen>:
    inertial: inertial
    ekg: ekg
    eeg: eeg
    staging: staging
    BoxLayout:
        orientation: 'vertical'
        background_color: 1,1,1,1

        BoxLayout:
            CtaButton:
                text: 'start'
                width: 250
                height: 50
                on_release: root.run()
            CtaButton:
                text: 'STOP'
                width: 250
                height: 50
                on_release: root.stop()

        GridLayout:
            cols: 2
            rows: 2
            StagingGraph:
                id: staging

            EegGraph:
                id: eeg

            InertialGraph:
                id: inertial

            EkgGraph:
                id: ekg




        Button:
            text: 'annulla'
            width: 250
            height: 50
            on_release: app.root.ids['sm'].current = 'homepage_screen'






