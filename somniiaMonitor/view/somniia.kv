#: kivy 2.3.0

#:include somniiaMonitor/view/includeAll.kv

#:import tr somniiaMonitor.view.somniia_ui.tr

#root = BoxLayout
BoxLayout:
    orientation: 'vertical'
    canvas.before:
        Color:
            rgba: 0.5, 0.5, 0.5, 1
        Rectangle:
            size: self.size
            pos: self.pos
    #I wont screen sit here
    ScreenManager:
        id: sm
        HomepageScreen:
            name: 'homepage_screen'
        DevicesScreen:
            name:'devices_screen'
        AnalysisScreen:
            name:'analysis_screen'



