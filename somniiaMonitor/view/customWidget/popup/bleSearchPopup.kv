#: kivy 2.3.0



<BleSearchPopup@Popup>:
    foundedBleList: foundedBleList
    title: 'Add a device'
    size_hint: .3, .8
    BoxLayout:
        orientation: 'vertical'

        CtaButton:
            id: 'search'
            text: 'search'
            size_hint_y: None
            height: 50
            on_release: root.search()


        BoxLayout:
            orientation: 'vertical'
            id: foundedBleList

        Button:
            text: 'cancel'
            size_hint_y: None
            height: 50
            on_release: root.dismiss()
